{
  "name": "nachotime",
  "version": "0.1.0",
  "description": "Stream movies from torrents with subtitles",
  "author": "benf3632",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "electron-squirrel-startup": "^1.0.0",
    "opensubtitles-api": "^5.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-infinite-scroller": "^1.2.4",
    "react-loader-spinner": "^4.0.0",
    "react-loading-skeleton": "^2.2.0",
    "react-pro-sidebar": "^0.7.1",
    "react-rating-stars-component": "^2.2.0",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-stonecutter": "^0.3.10",
    "web-vitals": "^1.1.2",
    "webtorrent": "^1.5.8"
  },
  "homepage": "./",
  "main": "electron/main.js",
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:start-electron\"",
    "start-electron": "wait-on tcp:3000 && ELECTRON_START_URL=http://localhost:3000 electron .",
    "build-electron": "cp -r electron/. build/electron",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-mac": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "wait-on": "^6.0.0"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "nachotime",
      "owner": "benf3632"
    },
    "win": {
      "target": "msi",
      "icon": "electron/icon.png"
    }
  }
}
